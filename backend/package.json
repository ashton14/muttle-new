{
  "name": "muttle-backend",
  "version": "0.1.0",
  "description": "NodeJS/Express backend server for muttle web application",
  "author": "Jon Lai",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start-dev": "ts-node-dev src/index.ts",
    "build": "yarn run test && yarn run compile",
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed:config": "node --require ts-node/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:drop"
  },
  "dependencies": {
    "express": "^4.17.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.30",
    "typeorm-seeding": "^1.6.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.11.2",
    "@types/xml2js": "^0.4.8",
    "gts": "^3.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.0.3"
  }
}
